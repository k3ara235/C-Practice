bool isitprime(int num){
struct prime{
int one;
int two = 1;
};
prime number;
number.one=num;
if(number.one >2 && number.one %2!=0){
  for(int i =0; i<number.one; i++){
    if(number.one%i==0){
    return false;
    }else{ return true;
    }
    }
    }
  if (number.one<3){
  return false;
  }

